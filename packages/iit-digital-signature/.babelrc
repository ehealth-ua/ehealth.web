{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "shippedProposals": true,
        "exclude": ["transform-regenerator", "transform-async-to-generator"],
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": ["module:fast-async", "@babel/plugin-proposal-class-properties"]
}
